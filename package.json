{
  "name": "rspack-starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production rspack build --mode production",
    "build:uat": "cross-env NODE_ENV=development rspack build --mode development",
    "dev": "cross-env NODE_ENV=development rspack serve --mode development",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "yarn build && serve dist/production",
    "preview:uat": "yarn build:uat && serve dist/uat"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "react-toastify": "^11.0.2",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@rspack/cli": "1.2.0-beta.0",
    "@rspack/core": "1.2.0-beta.0",
    "@rspack/plugin-react-refresh": "1.0.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-redux": "^7.1.34",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "react-refresh": "^0.14.0",
    "reset-css": "^5.0.2",
    "sass-embedded": "^1.83.1",
    "sass-loader": "^16.0.4",
    "serve": "^14.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  }
}
